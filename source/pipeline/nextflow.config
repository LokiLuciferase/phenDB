

params {
    inputfolder = ''
    modelfolder = '/apps/PICA/models_devel'
    workdir     = ''

    hmmdb                  = '/var/www/hmmdb/db/db.hmm'
    balanced_accuracy_path = "/apps/phenDB_devel_LL/source/pipeline/scripts/get_balanced_accuracy.py"
    description_file       = '/apps/phenDB_devel_LL/docs/PICA_descriptions.txt'
    accuracy_cutoff        = 0.75
    pica_conf_cutoff       = 0.75
    show_everything        = false
    max_bin_size           = 30000000
    omit_nodes             = ""
    omit_models            = []
    pica_dependencies      = '/apps/phenDB_devel_LL/source/pipeline/trait_dependencies.tsv'
}

profiles {

    standard {
        process.executor = 'local'
    }

    cluster {
        process.executor = 'slurm'
        trace.enabled    = 'true'
        trace.file       = 'phenDB_trace.txt'
    }

}
